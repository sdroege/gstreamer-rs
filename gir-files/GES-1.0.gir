            <type name="Clip" c:type="GESClip*"/>
          </instance-parameter>
          <parameter name="position" transfer-ownership="none">
            <doc xml:space="preserve">The timeline position at which to perform the split</doc>
            <type name="guint64" c:type="guint64"/>
          </parameter>
        </parameters>
      </method>
